<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip"/>
            <gradient
                android:angle="270"
                android:centerColor="#f2dfdddd"
                android:centerY="0.75"
                android:endColor="#ebeceb"
                android:startColor="#f4f2f2"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip"/>
                <gradient
                    android:angle="270"
                    android:centerColor="#6009b331"
                    android:centerY="0.75"
                    android:endColor="#6004961f"
                    android:startColor="#600e8447"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip"/>
                <gradient
                    android:angle="270"
                    android:centerColor="#09ec49"
                    android:centerY="0.75"
                    android:endColor="#06dc46"
                    android:startColor="#08da19"
                    />
            </shape>
        </clip>
    </item>

</layer-list>
